{
  "name": "sym_v4",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Make music by drawing shapes",
  "author": "Elias Jarzombek",
  "keywords": [
    "music",
    "shapes",
    "polygons",
    "web audio",
    "geometry"
  ],
  "private": false,
  "dependencies": {
    "@sentry/browser": "^5.0.8",
    "chalk": "^2.4.2",
    "classnames": "^2.2.5",
    "color": "^2.0.1",
    "faunadb": "^2.6.1",
    "file-saver": "2.0.0",
    "inline-worker": "1.1.0",
    "jszip": "3.1.5",
    "konva": "^3.3.1",
    "lodash": "^4.17.13",
    "midi-writer-js": "1.5.2",
    "netlify-identity-widget": "^1.5.2",
    "prop-types": "^15.6.0",
    "radium": "^0.24.0",
    "rc-select": "^7.7.6",
    "rc-slider": "^8.6.1",
    "react": "~16.8.6",
    "react-color": "^2.14.0",
    "react-dom": "~16.8.6",
    "react-draggable": "^3.0.3",
    "react-full-screen": "0.0.2",
    "react-fullscreenable": "^2.5.0",
    "react-hotkeys": "^1.1.4",
    "react-konva": "~16.8.6",
    "react-portal": "^3.2.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts-cssmodules": "^1.1.50",
    "react-select": "^1.2.1",
    "readline": "^1.3.0",
    "teoria": "^2.5.0",
    "tone": "^13.8.12",
    "unmute": "^0.1.3"
  },
  "scripts": {
    "checkForFaunaKey": "node ./scripts/check-for-fauna-key.js",
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "start": "npm-run-all --parallel start:app start:server",
    "start:app": "react-scripts start",
    "start:server": "yarn checkForFaunaKey && yarn bootstrap && netlify-lambda serve functions -c ./webpack.config.js",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:functions": "netlify-lambda build functions -c ./webpack.config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-prettier": "^2.6.2",
    "gh-pages": "^1.2.0",
    "netlify-lambda": "^1.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.1"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
